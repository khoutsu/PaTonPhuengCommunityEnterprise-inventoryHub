{
  "name": "ecom-warehouse",
  "version": "1.0.0",
  "description": "E-commerce Warehouse Management System",
  "scripts": {
    "dev": "docker-compose -f docker-compose.dev.yml up --build",
    "dev:detach": "docker-compose -f docker-compose.dev.yml up --build -d",
    "dev:down": "docker-compose -f docker-compose.dev.yml down",
    "prod": "docker-compose up --build",
    "prod:detach": "docker-compose up --build -d",
    "prod:down": "docker-compose down",
    "build": "docker-compose build",
    "build:dev": "docker-compose -f docker-compose.dev.yml build",
    "logs": "docker-compose logs -f",
    "logs:backend": "docker-compose logs -f backend",
    "logs:frontend": "docker-compose logs -f frontend",
    "clean": "docker-compose down -v --rmi all --remove-orphans",
    "clean:dev": "docker-compose -f docker-compose.dev.yml down -v --rmi all --remove-orphans"
  },
  "keywords": [
    "ecommerce",
    "warehouse",
    "docker",
    "nextjs",
    "nodejs"
  ],
  "author": "",
  "license": "ISC"
}